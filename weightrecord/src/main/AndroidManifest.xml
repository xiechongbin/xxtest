<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaoxie.weightrecord">

    <application
        android:name=".application.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_arrow_back_white"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.introActivity" />
        <activity android:name=".activity.SettingsActivity" />
        <activity
            android:name=".activity.LockPatternActivity"
            android:theme="@style/AppTheme" />
        <!-- <activity android:name=".activity.TestActivity"></activity> -->
        <activity android:name=".activity.TrendDetailsActivity"></activity>
    </application>

</manifest>